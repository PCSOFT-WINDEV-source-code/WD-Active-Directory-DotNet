      -
        internal_properties : CAAAAAgAAAB3R1fp360sBjl73Lfk/mVbM/275YP1bEmE0GbBuRTUJox5MraN+BkF91JEi/flTpPxXo+Hkhg+1j7ZyS43Bk2fCoBEf47lSltMUDY/HljC9U5b4ZNUpCEQW2FPHyr+VA+DmT9P/mPpbzeIxpAISrMWYiqOgdz4lNPIA3RRjnqZUkBEljASw5huD+4OfU4MNmXrmkhgFExg/zkSgtDutH09n5BwUEb56s1svyE7G7HyBhDnrTi4xYPk8C3ibjz4iiJpP2lKyKhBz1+EryQ=
        code : |1-
         IF EDT_DOMAIN = "" THEN
         	Error(<§$0010§>)
         	ReturnToCapture(EDT_DOMAIN)
         END
         
         // Deletes the full content from the TreeViews and from the table
         TableDeleteAll(TABLE_ITEM)
         TreeDeleteAll(TREE_Hidden)
         TreeDeleteAll(TREE_Item)
         
         
         HourGlass(True)
         
         sAddress is string
         
         // Build the connection address
         sAddress = "LDAP://" + EDT_DOMAIN
         
         // Checks the existence of ActiveDirectory at the specified address
         WHEN EXCEPTION IN
         	
         	DirectoryEntry::Exists(sAddress)
         	
         DO
         	// If an exception occurs, then no active directory is found at this address
         	// No ActiveDirectory is accessible
         	Error(<§$0012§>)
         	HourGlass(False)
         	RETURN
         END
         
         
         // Declare the .NET DirectoryEntry object (An Entry of activeDirectory)
         clObjDirEntry 			is DirectoryEntry(sAddress, EDT_USER, EDT_PWD, AuthenticationTypes::Secure) 
         clFindDirectory 	is DirectorySearcher(clObjDirEntry)	// NET object (DirectorySearcher) used to perform searches in the Active Directory
         pclResultList 		is SearchResultCollection dynamic	// .NET object (SearchResultCollection) used to retrieve all the search results
         pclResult 			is SearchResult dynamic			// .NET object (SearchResult) that corresponds to ONE result
         
         i 			is int		// Counter
         j 			is int = 1	// Counter
         sItemName 	is string		// Name of the item found
         sNode 		is string		// Node where the item will be added
         
         WHEN EXCEPTION IN
         	// Retrieve all the results
         	pclResultList = clFindDirectory:FindAll()
         DO
         	// Unable to connect. At this point, the Active Directory exists, so wrong password or user name
         	Error(<§$0013§> + EDT_DOMAIN, <§$0014§>)
         	// Removes the hourglass
         	HourGlass(False)
         	RETURN
         END
         
         nBound		is int = pclResultList:Count -1
         // Loop as long as results are found
         FOR i = 0 TO nBound
         	
         	// Retrieve the current item
         	pclResult = pclResultList:get_Item(i)
         	
         	// Reset the variables
         	sNode = ""
         	j=1
         	FOR EACH string sItemName of pclResult:path SEPARATED BY ","
         		
         		// Checks the position of the server name	
         		IF Position(sItemName,sAddress + "/") <> 0 THEN
         			// Point to the first one
         			
         			
         			// Add the item to the cache TreeView by removing the first 3 characters (CN= or OU= or DC=)
         			TreeAdd(TREE_Item,sNode+Replace(sItemName,sAddress + "/","")[[ 4 TO]],tvDefault,tvDefault,"")
         			TreeAdd(TREE_Hidden,sNode+Replace(sItemName,sAddress + "/","")[[ 4 TO]],tvDefault,tvDefault,"")
         			
         			// Replace the address of the server by an empty string
         			sNode += Replace(sItemName,sAddress + "/","")[[ 4 TO]]
         			
         			// Retrieve the properties
         			GetProperties(pclResult,sNode)
         			
         			BREAK
         		ELSE
         			// Adds to the cache TreeView
         			TreeAdd(TREE_Hidden,sNode+sItemName[[4 TO ]],tvDefault,tvDefault,"")
         			
         			// Remove the first 3 characters that are CN= or DN= or XX=
         			sNode += sItemName[[4 TO]]
         			
         			// Retrieve the properties
         			GetProperties(pclResult,sNode)
         			
         			// Adds to the visible TreeView
         			TreeAdd(TREE_Item,sNode,tvDefault,tvDefault,"",tvAlphaSort)
         			
         			// Add the TAB character to the node
         			sNode += TAB
         			
         		END				
         		
         	END
         END
         
         TreeExpand(TREE_Item,TREE_Item[1])
         TreeSelectPlus(TREE_Item,TREE_Item[1])
         
         // Refresh the properties
         ExecuteProcess(TREE_Item,trtSelection)
         
         HourGlass(False)
